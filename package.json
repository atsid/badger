{
  "name": "ats-badger",
  "version": "0.1.0",
  "description": "Github Badge Aggregator",
  "homepage": "https://github.com/atsid/badger",
  "scripts": {
    "serve:client": "webpack-dev-server --hot --inline --history-api-fallback --publicPath /client/",
    "install-git-hooks": "./scripts/npm/install-git-hooks.sh",
    "postinstall": "npm run install-git-hooks",
    "lint": "eslint {src,test}/**/*.js",
    "start": "npm run serve:clientv",
    "verify": "npm-run-all lint",
    "test": "npm run verify && gulp && gulp archive",
    "develop": "gulp develop",
    "report-coverage": "codeclimate-test-reporter < target/test-reports/all/lcov.info"
  },
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">=4.2.2 <5.x"
  },
  "bugs": "https://github.com/atsid/badger/issues",
  "author": {
    "name": "Chris Trevino",
    "email": "chris.trevino@atsid.com",
    "url": ""
  },
  "files": [
    "server",
    "config"
  ],
  "keywords": [],
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.2.0",
    "codeclimate-test-reporter": "^0.4.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "happypack": "^3.0.1",
    "ignore-loader": "^0.1.2",
    "isparta": "^4.0.0",
    "jsdom": "^9.9.1",
    "livereactload": "^3.1.1",
    "mkdirp": "^0.5.1",
    "nock": "^9.0.2",
    "npm-run-all": "^3.1.2",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "rewire": "^2.3.4",
    "run-sequence": "^1.1.2",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.0",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "supertest-session": "^2.0.1",
    "url-loader": "^0.5.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "bcryptjs": "^2.2.1",
    "bluebird": "^3.4.7",
    "debug": "^2.2.0",
    "express": "^4.13.1",
    "history": "^3.x",
    "lodash": "^4.17.3",
    "material-ui": "^0.16.6",
    "react": "^15.4.1",
    "react-addons-create-fragment": "^15.4.1",
    "react-addons-pure-render-mixin": "^15.4.1",
    "react-addons-transition-group": "^15.4.1",
    "react-addons-update": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "superagent": "^3.3.1",
    "superagent-bluebird-promise": "^4.1.0"
  },
  "browserify-shim": {
    "debug": "global:debug"
  },
  "aliasify": {
    "aliases": {
      "react": "./node_modules/react/react.js",
      "underscore": "lodash"
    }
  }
}
